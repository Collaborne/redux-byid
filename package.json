{
  "name": "redux-byid",
  "version": "0.4.3",
  "description": "Redux helper to define a \"by-id\" map",
  "main": "dist/index.js",
  "module": "dist.es6/index.js",
  "scripts": {
    "build": "tsc && tsc -p tsconfig.es6.json",
    "lint": "tslint -p . -c tslint.json *.ts",
    "test": "env TS_NODE_CACHE=false TS_NODE_FILES=true $(npm bin)/nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Collaborne/redux-byid.git"
  },
  "keywords": [
    "redux",
    "byid",
    "map"
  ],
  "author": "Collaborne <opensource@collaborne.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Collaborne/redux-byid/issues"
  },
  "homepage": "https://github.com/Collaborne/redux-byid#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "redux": "^4.0.1",
    "ts-node": "^8.10.2",
    "tsconfig-collaborne": "^1.3.0",
    "tslint": "^6.1.2",
    "tslint-config-collaborne": "^1.0.0",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "redux": "^4.0.1"
  },
  "files": [
    "index.ts",
    "tslint.json",
    "tsconfig*.json",
    "dist",
    "dist.es6"
  ],
  "mocha": {
    "extension": [
      "ts"
    ],
    "spec": "test/**/*.spec.ts",
    "recursive": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "interface": "bdd"
  },
  "nyc": {
    "exclude": [
      "**/*.d.ts",
      "dist/",
      "dist.*/",
      "test/",
      "reports/",
      "stryker.conf.js"
    ],
    "all": true
  }
}
